{
    "extends": "airbnb",
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 1,
        "no-unused-vars": 0,
        "linebreak-style": ["error", "windows"],
        "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
        "react/prefer-stateless-function": 1,
        "react/forbid-prop-types": 0
    },
    "env": {
        "browser": true
    }
}